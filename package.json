    "react": "^16.12.0",
    "react-compound-slider": "^2.5.0",
    "react-content-loader": "^5.0.2",
    "react-dom": "^16.12.0",
    "react-hook-inview": "^4.1.3",
    "react-multi-carousel": "^2.2.1",
    "react-paginate": "^6.3.2",
