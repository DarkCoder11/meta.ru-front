{
  "name": "metamovies-client",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "start": "next start",
    "build": "next build",
    "serve": "npm run build && npm run dev",
    "build-docker": "docker build -t next-app .",
    "analyze": "cross-env ANALYZE=true npm run build",
    "run-docker": "docker run --rm -it -p 3000:3000 next-app",
    "start:staging": "cross-env NODE_ENV=production ENV=staging next",
    "start:production": "cross-env NODE_ENV=production ENV=production next",
    "lint": "eslint \"**/*.js\" --fix --max-warnings=0 --ignore-pattern node_modules/",
    "build-docker-multistage": "docker build -t next-app -f ./Dockerfile.multistage .",
    "export": "cross-env NEXT_EXPORT=true next build && cross-env NEXT_EXPORT=true next export"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "resolutions": {
    "styled-jsx": "3.1.2"
  },
  "dependencies": {
